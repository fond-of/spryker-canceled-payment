{
  "name": "fond-of-spryker/canceled-payment",
  "description": "Supports canceled payments initiated by customer",
  "license": "MIT",
  "authors": [
    {
      "name": "Chris E",
      "email": "christopher.egger@fondof.de"
    }
  ],
  "scripts": {
    "test": "make codeception",
    "phpcs": "make phpcs",
    "phpstan": "make phpstan"
  },
  "require": {
    "php": ">=7.2",
    "spryker/checkout-extension": "^1.2.0",
    "spryker-shop/checkout-page": "^2.0.0 || ^3.0.0",
    "spryker/quote": "^1.0.0 || ^2.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "^0.12",
    "codeception/codeception": "<4.1.2",
    "codeception/mockery-module": "^0.2",
    "mikey179/vfsstream": "^1.6",
    "phpro/grumphp": "^0.14",
    "sebastian/phpcpd": "^4.0",
    "php-coveralls/php-coveralls": "^2.1",
    "fond-of-codeception/spryker": "dev-master",
    "phpstan/phpstan": "^0.12.38"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfSpryker\\": "src/FondOfSpryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfSpryker\\": "tests/FondOfSpryker/",
      "Generated\\": "src/Generated/"
    }
  }
}
